{
  "name": "angular-lotto",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --watch=false --code-coverage",
    "test:open-coverage": "http-server -c-1 ./coverage",
    "lint:check": "ng lint",
    "lint:fix": "ng lint --fix",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "build:prod": "ng build --configuration production --aot",
    "start:prod": "http-server ./dist/angular-lotto/browser -p 4200 c-1",
    "build-and-start:prod": "run-s build:prod start:prod",
    "e2e": "start-server-and-test build-and-start:prod http://localhost:4200 cypress:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/service-worker": "^18.2.0",
    "@ngrx/effects": "^18.0.2",
    "@ngrx/store": "^18.0.2",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "18.3.0",
    "cypress": "^13.14.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ngx-translate-testing": "^7.0.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "start-server-and-test": "^2.0.5",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.1.0"
  }
}
